<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="8dp">

    <TextView
        android:id="@+id/instituteTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Institute"
        android:textSize="16sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/programTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Program"
        android:textSize="14sp"/>

    <TextView
        android:id="@+id/quotaTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Quota"
        android:textSize="14sp"/>

    <TextView
        android:id="@+id/seatTypeTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Seat Type"
        android:textSize="14sp"/>

    <TextView
        android:id="@+id/genderTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Gender"
        android:textSize="14sp"/>

    <TextView
        android:id="@+id/openingRankTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Opening Rank"
        android:textSize="14sp"/>

    <TextView
        android:id="@+id/closingRankTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Closing Rank"
        android:textSize="14sp"/>

    <TextView
        android:id="@+id/yearTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Year"
        android:textSize="14sp"/>

    <TextView
        android:id="@+id/roundTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Round"
        android:textSize="14sp"/>
</LinearLayout>
